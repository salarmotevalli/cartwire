{
    "name": "salarmotevalli/cartwire",
    "description": "This package provide a cart for your laravel-livewire projects",
    "license": "MIT",
    "keywords": ["laravel", "tailwind", "cart"],
    "autoload": {
        "psr-4": {
            "Cartwire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cartwire\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "salar motevalli",
            "email": "salar.mo.ro@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^8.0|^9.0",
        "laravel/framework": "^8.0|^9.0",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "laravel/pint": "^1.0"
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "Cartwire\\CartwireServiceProvider",
                "Cartwire\\CartwireRouteServiceProvider"
            ]
        }
    }
}
