{
    "name": "salarmotevalli/cartwire",
    "description": "This package provide a cart for your laravel-livewire project",
    "license": "MIT",
    "keywords": ["laravel", "tailwind", "cart"],
    "autoload": {
        "psr-4": {
            "Salarmotevalli\\CartWire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Salarmotevalli\\CartWire\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "salar motevalli",
            "email": "salar.mo.ro@gmail.com"
        }
    ],
    "require": {
        "livewire/livewire": "^2.0"
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "Salarmotevalli\\CartWire\\CartwireServiceProvider",
                "Salarmotevalli\\CartWire\\CartwireRouteServiceProvider"
            ]
        }
    }
}
